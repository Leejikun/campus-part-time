<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/android_layout_bg">

    <HorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="horizontal"
        android:background="#ffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="127dp"
            android:background="@drawable/bg_mf">

        </LinearLayout>
        <com.example.administrator.practicaltraining.CircleImageView
            android:layout_width="104dp"
            android:layout_height="104dp"
            android:layout_centerHorizontal="true"
            android:src="@drawable/touxiang1"
            app:border_color="#ffffff"
            android:id="@+id/touxiang"
            android:layout_marginTop="75.5dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentBottom="true">
        </LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/mypt"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingTop="10dp"
        android:background="#ffffff">
        <TextView
            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="我的兼职"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/go"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        style="@style/PersonalLine"
        android:layout_below="@+id/id_myself_myshop"/>
    <RelativeLayout
        android:id="@+id/id_myself_publish"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingTop="10dp"
        android:background="#ffffff">
        <TextView
            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="发布兼职"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/go"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        style="@style/PersonalLine"
        android:layout_below="@+id/id_myself_myshop"/>
    <RelativeLayout
        android:layout_marginTop="@dimen/item_margin_top"
        android:id="@+id/share"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentTop="true">
        </LinearLayout>
        <TextView
            android:layout_marginTop="@dimen/item_margin_top"
            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分享"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentBottom="true">
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_marginTop="@dimen/item_margin_top"
        android:id="@+id/callme"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentTop="true">
        </LinearLayout>
        <TextView
            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="联系客服"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <ImageView
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:src="@drawable/call"
            android:layout_toLeftOf="@+id/id_myself_callnumber"
            android:layout_marginTop="18dp"
            android:layout_marginRight="1dp" />
        <TextView
            android:layout_centerInParent="true"
            android:id="@+id/id_myself_callnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1234567890"
            android:textSize="15dp"
            android:layout_marginRight="10dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        style="@style/PersonalLine"
        android:layout_below="@+id/id_myself_myshop"/>
    <RelativeLayout
        android:id="@+id/yijianfankui"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:paddingTop="10dp"
        android:background="#ffffff">
        <TextView

            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/go"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        style="@style/PersonalLine"
        android:layout_below="@+id/id_myself_myshop"/>
    <RelativeLayout
        android:layout_marginTop="@dimen/item_margin_top"
        android:id="@+id/forus"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentTop="true">
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#ffccccaa"
            android:layout_alignParentBottom="true">
        </LinearLayout>
        <TextView
            android:layout_marginLeft="20.0dip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于我们"
            android:textSize="17dp"
            android:layout_centerVertical="true"
            android:textColor="#000000"/>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/go"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:paddingRight="10dp"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"/>
    <Button
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:id="@+id/id_myself_tcbtn"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#ff0000"
        android:textColor="#ffffff"
        android:text="退出登录"
        android:textSize="17dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="15dp"></LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>